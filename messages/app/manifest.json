{
    "name": "Messages For Web",
    "description": "Open Google Messages For Web",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "icons": {
        "16": "icon/icon-16x16.png",
        "128": "icon/icon-144x144.png"
    },
    "permissions": ["webview","*://*.google.com/*","notifications"]
}